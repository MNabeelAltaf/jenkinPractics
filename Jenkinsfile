pipeline{
  
  agent any
  
  stages{
    
    stage("build"){
      
      steps{
        
       echo 'python file1_main_code.py'
        
      }
      
    }
    
       stage("test"){
      
      steps{
        
        echo  'python file2_test_scripts.py'
        
      }
      
    }
    
      stage("deploy"){
      
      steps{
        
       echo  'python file3_deployment.py'
        
      }
      
    }
  
  }
  
}
